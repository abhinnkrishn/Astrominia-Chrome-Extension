{
  "name": "Astrominia",
  "author": "Abhinn krishn",
  "description": "Beautiful Astronomical New Tab Pages.",
  "version": "0.1.0",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": ["tabs"],
  "background": {
    "scripts": ["script.js"],
    "persistent": false
  }
}
